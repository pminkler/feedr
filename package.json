{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:format": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "typecheck:e2e": "tsc --noEmit --project e2e/tsconfig.json",
    "check": "npm run lint:fix && npm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:verify-failures": "node tests/verify-test-failures.cjs",
    "test:e2e": "playwright test --project chromium --max-failures=1 --reporter=list",
    "test:e2e:ci": "playwright test --project chromium --max-failures=1 --reporter=line",
    "test:e2e:debug": "DEBUG_MODE=true playwright test --project chromium --max-failures=10 --retries=0 --timeout=60000",
    "test:e2e:fast": "playwright test --project chromium --reporter=line",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.13.1",
    "@aws-sdk/client-s3": "^3.738.0",
    "@aws-sdk/client-ses": "^3.777.0",
    "@aws-sdk/client-sfn": "^3.738.0",
    "@aws-sdk/client-textract": "^3.738.0",
    "@nuxt/ui-pro": "^3.0.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^9.3.1",
    "@pinia/nuxt": "^0.10.1",
    "@sentry/nuxt": "^9.9.0",
    "@sentry/vue": "^9.9.0",
    "aws-amplify": "^6.12.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "fraction.js": "^5.2.1",
    "nuxt": "^3.16.0",
    "nuxt-gtag": "^3.0.2",
    "openai": "^4.81.0",
    "pinia": "^3.0.1",
    "vue": "latest",
    "vue-router": "latest",
    "yup": "^1.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.14.3",
    "@aws-amplify/backend-cli": "^1.5.0",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/test-utils": "^3.17.2",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.0.9",
    "@vue/test-utils": "^2.4.6",
    "aws-cdk": "^2.177.0",
    "aws-cdk-lib": "^2.177.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "pm2": "^6.0.5",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "vue-tsc": "2.2.2",
    "wait-on": "^7.2.0"
  },
  "resolutions": {
    "vue-tsc": "2.2.2"
  },
  "overrides": {
    "@vercel/nft": "^0.27.4"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint --fix"
  }
}
